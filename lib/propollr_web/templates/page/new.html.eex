New Session!

<%= form_for @changeset, page_path(@conn, :add_question, @session_id), fn f -> %>

<label for="question">
  Question: <%= text_input f, :text %>
</label>

<label for="answer1">
  Answer 1: <%= text_input f, :answer1 %>
</label>

<label for="answer2">
  Answer 2: <%= text_input f, :answer2 %>
</label>

<label for="answer3">
  Answer 3: <%= text_input f, :answer3 %>
</label>

<label for="answer4">
  Answer 4: <%= text_input f, :answer4 %>
</label>

<%= submit "Submit" %>

<% end %>